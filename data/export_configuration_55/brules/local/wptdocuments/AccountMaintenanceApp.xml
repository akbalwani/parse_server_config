<?xml version="1.0" encoding="UTF-8"?>
<brpackage xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="AccountMaintenanceApp.xml" version="1.0" xsi:noNamespaceSchemaLocation="businessrules.xsd">
	<rule name="Account Application" type="static" precedence="100">
        <condition>
            <expression>
                <item>
                    <attribute>EventType</attribute>
                    <comparator name="equal" />
                    <value>Application - Schedule</value>
                </item>
                <operator name="and" />
                <item>
                    <attribute>DXAGENT_APPLICATION_TYPE</attribute>
                    <comparator name="equal" />
                    <value>AccountTTL</value>
                </item>
            </expression>
        </condition>
        <action Order="yes">
            <inprocess-agent id="1" executeafter="" streamaccess="none" wait="yes" class="com.axway.st.server.accountretentionapp.agents.AccountMaintenanceAgent" >
                <params>
                    <param>
                        <param-name>Impersonation</param-name>
                        <param-value>"true"</param-value>
                    </param>
                </params>
            </inprocess-agent>
        </action>
    </rule>
</brpackage>